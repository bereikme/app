{
  "name": "$t:ssl-icon",
  "version": "1.0.0",
  "types": ["boolean"],
  "icon": "favorite_border",
  "options": {
    "textInactive": {
      "name": "Inactive",
      "comment": "$t:text-inactive-comment",
      "interface": "text-input",
      "options": {
        "placeholder": "off"
      }
    },
    "iconInactive": {
      "name": "$t:icon-inactive-name",
      "comment": "$t:icon-inactive-comment",
      "default": "favorite_border",
      "interface": "icon"
	  "icon": "block"
    },
    "colorInactive": {
      "name": "$t:color-inactive-name",
      "comment": "$t:color-inactive-comment",
      "default": "red0",
      "interface": "color-palette"
    },
    "textActive": {
      "name": "Active",
      "comment": "on",
	  "value": "1",
      "interface": "text-input",
      "options": {
        "placeholder": "on"
      }
    },
    "iconActive": {
      "name": "$t:icon-active-name",
      "comment": "$t:icon-active-comment",
      "default": "favorite",
      "interface": "icon"
    },
    "colorActive": {
      "name": "$t:color-active-name",
      "comment": "$t:color-active-comment",
      "default": "green",
      "interface": "color-palette",
	  "icon": "block"
    }
  }
}
